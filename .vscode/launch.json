    "version": "0.2.0",
    "configurations": [

        {
            "name": "LaunchServer",
            "type": "mono",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/ServerWorker/bin/Debug/ServerWorker.exe",
            "cwd": "${workspaceRoot}/ServerWorker/bin/Debug",
            "console": "integratedTerminal"
        },
        {
            "name": "LaunchClient",
            "type": "mono",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/ClientWorker/Service/bin/Debug/Service.exe",
            "cwd": "${workspaceRoot}/ClientWorker/Service/bin/Debug",
            "console": "integratedTerminal"
        },
        {
            "name": "Attach",
            "type": "mono",
            "request": "attach",
            "address": "localhost",
            "port": 55555,
        }
    ]
}